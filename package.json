{
  "name": "urnovl-web-components",
  "version": "0.0.15",
  "description": "Web components for Urnovl",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/urnovl-web-components/urnovl-web-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/urnovl-web-components/urnovl-web-components.esm.js",
      "require": "./dist/urnovl-web-components/urnovl-web-components.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@stencil/core": "^4.20.0",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/addon-storysource": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/test": "^8.2.7",
    "@storybook/web-components": "^8.2.7",
    "@storybook/web-components-vite": "^8.2.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.1.4",
    "puppeteer": "^22.15.0",
    "storybook": "^8.2.7",
    "storybook-addon-stencil": "^0.2.1",
    "typescript": "^5.5.4"
  },
  "license": "MIT"
}
